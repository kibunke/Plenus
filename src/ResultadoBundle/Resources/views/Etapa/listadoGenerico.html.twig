<div class="dropdown pull-right">
    <a data-toggle="dropdown" class="btn btn-xs dropdown-toggle btn-transparent-grey">
        <i class="fa fa-cog"></i>
    </a>
    <ul class="dropdown-menu dropdown-light pull-right" role="menu"></ul>
</div>
<div class="dropdown pull-right" style="margin-right: 5px;">
    <a data-toggle="dropdown" class="btn btn-xs dropdown-toggle btn-transparent-grey" aria-expanded="false">
        <i class="fa fa-print"></i>
    </a>
    <ul class="dropdown-menu dropdown-light pull-right" role="menu" style="display: none;"></ul>
</div>
<table id="equipoTable" class="table table-striped table-hover">
    <thead>
        <tr>
            <th class="text-center" style="width: 10px"></th>
            <th class="text-center">Regi√≥n</th>
            <th class="text-center">Muincipio</th>
            <th>Equipo/Participante</th>
            <th style="width: 95px"></th>
        </tr>
    </thead>
    <tbody>
        {% for equipo in etapa.equipos %}
            <tr data-plaza="{{ equipo.id }}">
                <td class="text-center"><i class="fa fa-ellipsis-v">&nbsp;&nbsp;</i></td>
                <td class="text-center"><b>{{ equipo.planilla.municipio.regionDeportiva }}</b></td>
                <td>{{ equipo.planilla.municipio.nombre }}</td>
                <td>{{ equipo | raw }}</td>
                <td class="text-center"></td>
            </tr>
        {% endfor %}
    </tbody>
</table>
