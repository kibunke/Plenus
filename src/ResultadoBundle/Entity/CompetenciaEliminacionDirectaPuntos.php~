<?php

namespace JuegosBA\ResultadoBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Doctrine\Common\Collections\ArrayCollection;

/**
 * JuegosBA\ResultadoBundle\Entity\CompetenciaEliminacionDirectaPuntos
 *
 * @ORM\Entity()
 *
 */
class CompetenciaEliminacionDirectaPuntos extends CompetenciaEliminacionDirecta
{ 
    /** 
     * Get newPartido
     *
     * @return \JuegosBA\ResultadoBundle\Entity\Partido
     */
    public function newPartido($user = null)
    {
        return new PartidoPuntos($user);
    }
}
